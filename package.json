{
  "name": "maylily",
  "version": "2.0.0",
  "description": "distributable, serverless, and customizable unique ID generator based on Snowflake",
  "main": "./dist/maylily.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shimataro/maylily.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "node.js",
    "javascript",
    "snowflake",
    "flake",
    "unique",
    "id",
    "uid",
    "twitter",
    "generator"
  ],
  "author": "shimataro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shimataro/maylily/issues"
  },
  "homepage": "https://github.com/shimataro/maylily",
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "npm run build-core",
    "build-core": "babel ./src --out-dir ./dist --source-maps",
    "watch": "run-p watch:*",
    "watch:js": "npm run build-core -- --watch",
    "verify": "run-p lint",
    "lint": "run-p lint:*",
    "lint:es": "eslint ./src --ext .es",
    "lint:md": "markdownlint . --ignore ./node_modules",
    "lint:yaml": "yamllint .*.yml *.yml --ignore=node_modules/**/*.yml --ignore=node_modules/**/*.yaml",
    "postinstall": "npm run build",
    "sample": "node dist/sample",
    "check-updates": "ncu"
  },
  "dependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "babel-plugin-add-module-exports": "1.0.0",
    "big-integer": "1.6.27"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.12.0",
    "eslint-plugin-import": "2.14.0",
    "markdownlint-cli": "0.13.0",
    "npm-check-updates": "2.14.1",
    "npm-run-all": "4.1.5",
    "yaml-lint": "1.2.4"
  }
}
