{
  "name": "maylily",
  "version": "2.0.0",
  "description": "distributable, serverless, and customizable unique ID generator based on Snowflake",
  "main": "./dist/maylily.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shimataro/maylily.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "node.js",
    "javascript",
    "snowflake",
    "flake",
    "unique",
    "id",
    "uid",
    "twitter",
    "generator"
  ],
  "author": "shimataro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shimataro/maylily/issues"
  },
  "homepage": "https://github.com/shimataro/maylily",
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "gulp build-js",
    "watch": "run-p watch:*",
    "watch:gulpfile": "gulp watch-gulpfile",
    "watch:js": "gulp watch-js",
    "lint": "run-p lint:*",
    "lint:md": "markdownlint . --ignore ./node_modules",
    "postinstall": "npm run build",
    "sample": "node dist/sample",
    "check-updates": "ncu"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "big-integer": "^1.6.27",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0"
  },
  "devDependencies": {
    "markdownlint-cli": "^0.13.0",
    "npm-check-updates": "^2.14.1",
    "npm-run-all": "^4.1.5"
  }
}
